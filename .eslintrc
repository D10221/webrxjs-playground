{
    "rules": {
        "indent": [
            2,
            4
        ],
        "quotes": [
            2,
            "double"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ]
    },
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  } ,
    "extends": "eslint:recommended",
    "ecmaFeatures": {
        "jsx": true,
        "ecmaFeatures": {
            "modules": true
        }
    }
}